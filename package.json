{
  "name": "wspbclient",
  "version": "1.3.4",
  "description": "client side for wspbserver",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "patch-reader": "replace -s 'Uint8Array ? new pb_1.BinaryReader(bytes) : bytes' 'pb_1.BinaryReader ? bytes : new pb_1.BinaryReader(bytes)' --  src/*Proto.ts",
    "patch": "npm run patch-reader",
    "protots": "protoc -I src/proto --ts_out=src src/proto/*.proto",
    "testdet": "jest --runInBand --force-exit --verbose --detectOpenHandles",
    "test": "jest --runInBand --force-exit --verbose",
    "tsc-options": "are configured in tsconfig.json",
    "tsc": "tsc --esModuleInterop",
    "clean": "rm -f dist/out-tsc/*",
    "build": "npm run clean; npm run protots; npm run patch; npm run tsc; date"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackpunt/wsbpclient.git"
  },
  "keywords": [
    "websocket",
    "browser",
    "node",
    "client"
  ],
  "author": "Jeff Peck <jpeckj@gmail.com> (http://thegraid.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackpunt/wsbpclient/issues"
  },
  "homepage": "https://github.com/jackpunt/wsbpclient#readme",
  "dependencies": {
    "@thegraid/ezpromise": "^1.0.5",
    "google-protobuf": "^3.15.8",
    "moment": "^2.29.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.0",
    "@types/jest": "^26.0.22",
    "@types/ws": "^7.4.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "ws": "^7.4.5"
  }
}
